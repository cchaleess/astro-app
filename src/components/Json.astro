---
const response = await fetch("https://jsonplaceholder.typicode.com/users");
const data = await response.json();
---

<table>
        <th>Name</th>
        <th>Email</th>
        <th>Location</th>
        {
                data.map((item) => {
                        return (
                                <tr>
                                        <td>{item.name}</td>
                                        <td>{item.email}</td>
                                        <td>{item.address.city}</td>
                                </tr>
                        );
                })
        }
</table>
